									  <div class="mySlides">
									    <div class="numbertext">3 / 6</div>
									      <img src="<%= asset_path('patio.JPG') %>" style="width:100%">
									  </div>

									  <div class="mySlides">
									    <div class="numbertext">4 / 6</div>
									      <img src="<%= asset_path('driveway.JPG') %>" style="width:100%">
									  </div>

									  <div class="mySlides">
									    <div class="numbertext">5 / 6</div>
									      <img src="<%= asset_path('basement.JPG') %>" style="width:100%">
