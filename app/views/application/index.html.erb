									    <div class="numbertext">6 / 6</div>
									      <img src="<%= asset_path('foundation.jpg') %>" style="width:100%">
									  </div>

									  <!-- Next and previous buttons -->
									  <a class="prev" onclick="plusSlides(-1)">&#10094;</a>
									  <a class="next" onclick="plusSlides(1)">&#10095;</a>

									  <!-- Image text -->
									  <div class="caption-container">
									    <p id="caption"></p>
									  </div>

									  <!-- Thumbnail images -->
									  <div class="row demo-row">
									    <div class="column">
									      <img class="demo cursor active" src="<%= asset_path('stamped.jpg') %>" style="width:100%" onclick="currentSlide(1)" alt="Stamped">
									    </div>
									    <div class="column"> 
									      <img class="demo cursor" src="<%= asset_path('pooldeck.jpg') %>" style="width:100%" onclick="currentSlide(2)" alt="Pool Deck">
